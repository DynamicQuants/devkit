{
  "eslint.workingDirectories": [{ "mode": "auto" }],
  "editor.defaultFormatter": "esbenp.prettier-vscode",
  "typescript.referencesCodeLens.enabled": true,
  "editor.formatOnSave": true,
  "files.exclude": {
    "**/__pycache__/": true,
    "**/.pytest_cache/": true,
    ".mypy_cache/": true,
    "**/.coverage": true,
