{
  "name": "node-cli",
  "version": "0.0.0",
  "description": "Node CLI template",
  "keywords": [
    "cli",
    "node"
  ],
  "bin": {
    "node-cli": "./src/index.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "copy": "copyfiles -f README.md package.json dist && copyfiles ./files/**/* dist",
    "build": "pnpm clean && tsc -p tsconfig.app.json && pnpm copy",
    "lint": "eslint \"{src,tests}/**/*.{ts,json}\" --fix",
    "dev": "nodemon ./src/index.ts",
    "start": "node dist/src/index.js",
    "test": "jest --verbose",
    "test-watch": "jest --watch",
    "test-cov": "jest --coverage",
    "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
    "format-check": "prettier --check \"src/**/*.ts\" \"tests/**/*.ts\""
  },
  "dependencies": {
    "@octokit/rest": "^21.1.0",
    "chalk": "^4.1.2",
    "commander": "^12.1.0",
    "inquirer": "^12.2.0",
    "yaml": "^2.7.0"
  },
  "devDependencies": {
    "@dynamic-quants/configs": "workspace:*",
    "copyfiles": "^2.4.1",
    "nodemon": "^3.1.9",
    "rimraf": "^6.0.1"
  },
  "peerDependencies": {
    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.1",
    "jest": "^29.7.0",
    "tslib": "^2.8.1",
    "tsx": "^4.19.2",
    "typescript": "~5.6.3"
  }
}
